<div class="ship-to">
  <div class="label">{str_header_ship_to_label}</div>
  <div class="country">
    <div class="selected">
      <img class="lazy-image" src="{page_domain}{template_path}images/1px.gif" data-src="{page_domain}{template_path}images/flags-4x3/{country_iso_selected}.svg" width="1" height="1" alt="{country_selected}">
      <span>{country_selected}</span>
    </div>
    <div class="values"> 
      <div class="inner-values"> 
        <div class="select-label">{str_header_ship_to_country_of_delivery}</div>
        <div class="value ship-to-selection-change">{str_header_ship_to_change}</div>
      </div>
    </div>
  </div>
</div>
<section id="our-mission-and-goals">
  
  <div class="banner">
    <div class="container">
      <div class="inner-container">
        <div class="background">
          <div class="inner-background">
            <picture class="rwd-background rwd-background-overlay lazy-picture">
              <img src="{page_domain}{template_path}images/1px.gif" data-src="{page_domain}{template_path}images/our-mission-and-goals-banner.jpg" alt="Winderen">
            </picture>
          </div>
        </div>
        <div class="content logo-icon-with-label">
          <div class="logo">
            <img class="lazy-image" src="{page_domain}{template_path}images/1px.gif" data-src="{page_domain}{template_path}images/logo-only-icon.svg" alt="Winderen" width="34" height="48">
          </div>
          <h2>{str_homepage_our_mission_and_goals}</h2>
          <h3>{str_homepage_we_create_equestrian_equipment}</h3>
        </div>
      </div>
    </div>
  </div>

  <div class="content-about-our-mission">
    <div class="container">
      <div class="inner-container">
        <h2>{str_homepage_how_it_all_started_header}</h2>
        <div class="content">{str_homepage_how_it_all_started_text}</div>
      </div>
    </div>
  </div>

  <div class="show-hidden-content">
    <div class="container">
      <div class="inner-container">
        <span class="button-wrapper">
          <span class="icon">
            <img class="lazy-image" src="{page_domain}{template_path}images/1px.gif" data-src="{page_domain}{template_path}images/icons-book.svg" alt="Winderen" width="1" height="1">
          </span>
          <span class="icon">
            <img class="lazy-image" src="{page_domain}{template_path}images/1px.gif" data-src="{page_domain}{template_path}images/icons-book-gray.svg" alt="Winderen" width="1" height="1">
          </span>
          <span class="label">{str_learn_more}</span>
          <span class="circular-icon">
            <img class="lazy-image" src="{page_domain}{template_path}images/1px.gif" data-src="{page_domain}{template_path}images/icons-arrow-bottom-expand-white.svg" alt="{str_learn_more}" width="1" height="1">
            <img class="lazy-image" src="{page_domain}{template_path}images/1px.gif" data-src="{page_domain}{template_path}images/icons-arrow-bottom-expand-gray.svg" alt="{str_learn_more}" width="1" height="1">
          </span>
        </span>
        <span class="button-wrapper">
          <span class="label">{str_general_close}</span>
          <span class="circular-icon">
            <img class="lazy-image" src="{page_domain}{template_path}images/1px.gif" data-src="{page_domain}{template_path}images/icons-close.svg" alt="{str_general_close}" width="1" height="1">
            <img class="lazy-image" src="{page_domain}{template_path}images/1px.gif" data-src="{page_domain}{template_path}images/icons-close-gray.svg" alt="{str_general_close}" width="1" height="1">
          </span>
        </span>
      </div>
    </div>
  </div>

  <div class="hidden-content-about-our-mission">
    <div class="additional-content-about-our-mission">
      <div class="container">
        <div class="inner-container">
          
          <div class="general-outer">
            <div class="general">
              <div class="general-inner">
                <div class="logo">
                  <img class="lazy-image" src="{page_domain}{template_path}images/1px.gif" data-src="{page_domain}{template_path}images/logo-only-icon.svg" alt="Winderen" width="34" height="48">
                </div>
                <div class="text">{str_homepage_our_mission_general_text}</div>
              </div>
            </div>
          </div>

          <div class="items">

            <div class="item">
              <div class="column">
                <h2>{str_homepage_our_mission_general_items_1_title}</h2>
                <div class="text">{str_homepage_our_mission_general_items_1_text}</div>
              </div>
              <div class="column">
                <div class="image">
                  <picture class="lazy-picture">
                    <img src="{page_domain}{template_path}images/1px.gif" data-src="{page_domain}{template_path}images/our-mission-and-goals-image-1.jpg" alt="{str_homepage_our_mission_general_items_1_title}">
                  </picture>
                </div>
              </div>
            </div>

            <div class="item">
              <div class="column">
                <h2>{str_homepage_our_mission_general_items_2_title}</h2>
                <div class="text">{str_homepage_our_mission_general_items_2_text}</div>
              </div>
              <div class="column">
                <div class="image"> 
                  <picture class="lazy-picture">
                    <img src="{page_domain}{template_path}images/1px.gif" data-src="{page_domain}{template_path}images/our-mission-and-goals-image-2.jpg" alt="{str_homepage_our_mission_general_items_2_title}">
                  </picture>
                </div>
              </div>
            </div>

            <div class="item">
              <div class="column">
                <h2>{str_homepage_our_mission_general_items_3_title}</h2>
                <div class="text">{str_homepage_our_mission_general_items_3_text}</div>
              </div>
              <div class="column">
                <div class="image"> 
                  <picture class="lazy-picture">
                    <img src="{page_domain}{template_path}images/1px.gif" data-src="{page_domain}{template_path}images/our-mission-and-goals-image-3.jpg" alt="{str_homepage_our_mission_general_items_3_title}">
                  </picture>
                </div>
              </div>
            </div>

          </div>

          <div class="promo-item">
            <div class="promo-item-inner">
              <div class="column">
                <div class="image"> 
                  <picture class="lazy-picture">
                    <img src="{page_domain}{template_path}images/1px.gif" data-src="{page_domain}{template_path}images/our-mission-and-goals-image-4.png" alt="Winderen">
                  </picture>
                </div>
              </div>
              <div class="column">
                <div class="text">{str_homepage_our_mission_general_promo_text}</div>
              </div>
            </div>
          </div>

        </div>
      </div>
    </div>

    <div class="what-drives-us">
      <div class="background">
        <div class="inner-background">
          <picture class="rwd-background rwd-background-overlay lazy-picture">
            <img src="{page_domain}{template_path}images/1px.gif" data-src="{page_domain}{template_path}images/what-drives-us-banner.jpg" alt="Winderen">
          </picture>
        </div>
      </div>
    </div>

    <div class="what-drives-us-content">
      <div class="container">
        <div class="inner-container">
          <div class="content-outer">
            <div class="content">
              <div class="content-inner logo-icon-with-label">
                <div class="logo">
                  <img class="lazy-image" src="{page_domain}{template_path}images/1px.gif" data-src="{page_domain}{template_path}images/logo-only-icon.svg" alt="{str_homepage_what_drives_us_title}" width="34" height="48">
                </div>
                <h2>{str_homepage_what_drives_us_title}</h2>
                <div class="text">{str_homepage_what_drives_us_text}</div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="global-innovation-made-locally">
      <div class="container">
        <div class="inner-container">
          <div class="header logo-icon-with-label">
            <div class="icon">
              <div class="circular-icon">
                <img class="lazy-image" src="{page_domain}{template_path}images/1px.gif" data-src="{page_domain}{template_path}images/logo-global-innovation-made-locally.svg" alt="Winderen" width="48" height="48">
              </div>
            </div>
            <h2>{str_homepage_global_innovation_made_locally_title}</h2>
          </div>
          <div class="content">
            <div class="content-inner">
              <div class="column">
                <div class="text">{str_homepage_global_innovation_made_locally_text}</div>
              </div>
              <div class="column">
                <div class="image"> 
                  <picture class="lazy-picture">
                    <img src="{page_domain}{template_path}images/1px.gif" data-src="{page_domain}{template_path}images/logo-designed-and-manufactured-in-europe.png" alt="Winderen">
                  </picture>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>

  </div>

</section>